select *
from (select i_class_id, i_category_id, avg(ss_quantity*ss_list_price) sales, count(1) tot
from store_sales, item, date_dim
where ss_item_sk = i_item_sk
  and ss_sold_date_sk = d_date_sk
  and d_year = 2001
  and d_moy = 11
  and i_class_id is not null
  and i_category_id > 5
group by i_class_id, i_category_id) t
order by tot
