-- select max(tot), min(tot) 20515, 515
-- from (select i_category_id, count(1) as tot
-- from item
-- group by i_category_id) t

select i_brand_id, i_class_id, i_category_id, count(1)
from store_sales, item, date_dim
where ss_item_sk = i_item_sk
  and ss_sold_date_sk = d_date_sk
  and d_year = 2001
  and d_moy = 11
group by i_brand_id, i_class_id, i_category_id
