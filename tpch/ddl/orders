CREATE EXTERNAL TABLE orders (
	O_ORDERKEY INT
	,O_CUSTKEY INT
	,O_ORDERSTATUS STRING
	,O_TOTALPRICE DOUBLE
	,O_ORDERDATE STRING
	,O_ORDERPRIORITY STRING
	,O_CLERK STRING
	,O_SHIPPRIORITY INT
	,O_COMMENT STRING
	) ROW FORMAT DELIMITED FIELDS TERMINATED BY '|' STORED AS TEXTFILE LOCATION ':root/orders'
